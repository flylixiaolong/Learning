#include <stdio.h>
#include <stdlib.h>

struct Node{
	struct Node *next;
	int num;
};

static struct Node *list;

static struct Node **find(int num)
{
	struct Node **p;
	for(p=&list;*p;*p=(*p)->next)
	{
		if((*p)->num==num)
			break;
	}
	return p;
}
int main()
{
	struct Node **p,*e;
	p=find(0);
	if(!(*p))
	{
		*p=malloc(sizeof(struct Node*));
		(*p)->next=NULL;
		(*p)->num=0;
		printf("hello\n");
    }
	for(e=list;e;e=e->next)
	{
		printf("%d\n",e->num);
	}
	return 0;
}
